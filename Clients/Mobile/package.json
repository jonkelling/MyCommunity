{
    "name": "mycommunity",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "node node_modules/react-native/local-cli/cli.js start --transformer node_modules/react-native-sm-transformer --skipflow",
        "android-emulator": "$ANDROID_HOME/tools/emulator @Nexus_5X_API_23",
        "android": "adb reverse tcp:8081 tcp:8081 && node node_modules/react-native/local-cli/cli.js run-android",
        "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
        "prebuild": "rm -rf build",
        "build": "tsc",
        "bundle-android": "node node_modules/react-native/local-cli/cli.js bundle --dev false --entry-file index.android.prod.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
        "bundle-ios": "node node_modules/react-native/local-cli/cli.js bundle --dev false --entry-file index.ios.prod.js --bundle-output ios/main.jsbundle",
        "watch": "tsc -p . --watch",
        "test": "jest",
        "remotedevserver": "node remotedevserver.js"
    },
    "dependencies": {
        "@shoutem/ui": "^0.14.2",
        "@types/immutability-helper": "^2.0.15",
        "@types/lodash": "^4.14.64",
        "@types/react": "^15.0.23",
        "@types/react-native": "^0.43.7",
        "@types/react-redux": "^4.4.38",
        "@types/redux-actions": "^1.2.3",
        "@types/redux-saga": "^0.10.5",
        "immutability-helper": "^2.1.2",
        "json-stringify-safe": "^5.0.1",
        "jwt-decode": "^2.2.0",
        "linq": "^3.0.8",
        "lodash.isequal": "^4.5.0",
        "lodash.merge": "^4.6.0",
        "lodash.omit": "^4.5.0",
        "moment": "^2.18.1",
        "normalizr": "^3.2.2",
        "react": "16.0.0-alpha.6",
        "react-native": "^0.43.0",
        "react-native-blur": "^3.0.0",
        "react-native-button": "^1.7.1",
        "react-native-keychain": "^1.2.0",
        "react-native-lock": "^0.6.0",
        "react-native-navigation": "^1.1.61",
        "react-redux": "^5.0.4",
        "redux": "^3.6.0",
        "redux-actions": "^2.0.2",
        "redux-api-middleware": "github:jonkelling/redux-api-middleware#dist",
        "redux-logger": "^3.0.1",
        "redux-normalizr3-middleware": "^2.0.1",
        "redux-persist": "^4.8.0",
        "redux-saga": "^0.15.3",
        "redux-thunk": "^2.1.0",
        "typescript": "2.3.2",
        "uuid": "^3.1.0"
    },
    "devDependencies": {
        "babel-jest": "19.0.0",
        "babel-preset-react-native": "1.9.1",
        "jest": "19.0.2",
        "react-native-sm-transformer": "^1.0.3",
        "react-test-renderer": "16.0.0-alpha.6",
        "remote-redux-devtools": "^0.5.12",
        "remotedev-server": "^0.2.2"
    },
    "jest": {
        "preset": "react-native"
    }
}
